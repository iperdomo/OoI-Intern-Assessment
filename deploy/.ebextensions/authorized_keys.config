# Enables additional SSH access to instances
#
# To allow SSH access to additional people, add their public SSH keys here
# To remove SSH access from someone, remove their key from the list

files:
  /home/ec2-user/.ssh/authorized_keys:
    mode: "000400"
    owner: ec2-user
    group: ec2-user
    content: |
      ssh-rsa  awolski
      ssh-rsa  iperdomo
